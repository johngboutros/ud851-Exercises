<?xml version="1.0" encoding="utf-8"?>
<menu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--(2) Create a menu resource in res/menu/ called main.xml-->
    <!--(3) Add one item to the menu with an ID of action_refresh-->
    <!--(4) Set the title of the menu item to "Refresh" using strings.xml-->
    <!--(5) Set the orderInCategory value to 1 to make sure this item is the first in the list-->
    <!--(6) Set app:showAsAction to ifRoom to display the menu item in the ActionBar if there is room-->

    <item
        android:id="@+id/action_reset"
        android:orderInCategory="1"
        android:title="@string/reset"
        app:showAsAction="ifRoom" />


    <item
        android:id="@+id/action_reset_four"
        android:orderInCategory="2"
        android:title="@string/reset_four"
        app:showAsAction="ifRoom" />

</menu>